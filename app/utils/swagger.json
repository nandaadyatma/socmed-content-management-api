{
    "openapi": "3.0.0",
    "info": {
      "title": "ðŸ”— Socmed Content Management",
      "version": "1.0.0"
    },
    "servers": [
      {
        "url": "http://localhost:9000/api/v1"
      }
    ],
    "tags": [
      {
        "name": "Auth"
      }
    ],
    "paths": {
      "/auth/register": {
        "post": {
          "tags": [
            "default"
          ],
          "summary": "Register",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "example": {
                    "name": "Admin Nanda",
                    "email": "admin1@gmail.com",
                    "password": "Inipasswordnya12"
                  }
                }
              }
            }
          },
          "responses": {
            "400": {
              "description": "Bad Request",
              "headers": {
                "X-Powered-By": {
                  "schema": {
                    "type": "string",
                    "example": "Express"
                  }
                },
                "Access-Control-Allow-Origin": {
                  "schema": {
                    "type": "string",
                    "example": "*"
                  }
                },
                "Content-Type": {
                  "schema": {
                    "type": "string",
                    "example": "application/json; charset=utf-8"
                  }
                },
                "Content-Length": {
                  "schema": {
                    "type": "integer",
                    "example": "95"
                  }
                },
                "ETag": {
                  "schema": {
                    "type": "string",
                    "example": "W/\"5f-01Buo5iZnrB2ekPKbgGilG1++dc\""
                  }
                },
                "Date": {
                  "schema": {
                    "type": "string",
                    "example": "Wed, 19 Feb 2025 11:17:44 GMT"
                  }
                },
                "Connection": {
                  "schema": {
                    "type": "string",
                    "example": "keep-alive"
                  }
                },
                "Keep-Alive": {
                  "schema": {
                    "type": "string",
                    "example": "timeout=5"
                  }
                }
              },
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object"
                  },
                  "example": {
                    "error": true,
                    "message": "Duplicate value entered for email field, please choose another value"
                  }
                }
              }
            }
          }
        }
      },
      "/auth/login": {
        "post": {
          "tags": [
            "default"
          ],
          "summary": "Login",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "example": {
                    "name": "Admin Nanda",
                    "email": "admin1@gmail.com",
                    "password": "Inipasswordnya12"
                  }
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Successful response",
              "content": {
                "application/json": {}
              }
            }
          }
        }
      }
    }
  }